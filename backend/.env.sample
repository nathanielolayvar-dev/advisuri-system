# --- DJANGO SETTINGS ---
# Generate a fresh one for production or keep your current one for dev
DJANGO_SECRET_KEY=django-insecure-your-secret-key-here
DEBUG=True

# --- SUPABASE AUTHENTICATION ---
# Found in Supabase -> Settings -> API -> JWT Settings -> JWT Secret
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# --- SUPABASE POSTGRES DATABASE ---
# Found in Supabase -> Settings -> Database -> Connection Info
DB_NAME=postgres
DB_USER=postgres
DB_PWD=your_supabase_db_password_here
DB_HOST=db.yourprojectref.supabase.co
DB_PORT=5432

SUPABASE_URL="https://your-project-id.supabase.co"
SUPABASE_ANON_KEY="your-long-public-anon-key"